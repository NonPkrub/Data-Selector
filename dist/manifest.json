{
    "name":"Data Selector",
    "version": "1.0.0",
    "description":" Data Selector Web Component.",
    "permissions": [
        "activeTab",
        "downloads",
        "tabs",
        "scripting",
        "declarativeContent",
        "background",
        "http://*/*", 
        "https://*/*",
        "<all_urls>"
    ],
    "background":{
        "service_worker": "js/background.js"

    },
    "action":{
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content_scripts/content.js"]
        }
      ],
    "manifest_version": 3
}